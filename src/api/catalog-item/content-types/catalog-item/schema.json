        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "gallery": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "catalog_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::catalog-category.catalog-category"
    }
  }
}
